                                {
                                  "type": "option",
                                  "props": {
                                    "name": "What",
                                    "value": "Aspect.What"
                                  },
                                  "children": []
                                },
                                {
                                  "type": "option",
                                  "props": {
                                    "name": "When",
                                    "value": "Aspect.When"
                                  },
                                  "children": []
