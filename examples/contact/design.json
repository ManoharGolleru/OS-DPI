                            "direction": "column",
                            "background": "",
                            "scale": "1"
                          },
                          "children": [
                            {
                              "type": "radio",
                              "props": {
                                "label": "",
                                "stateName": "$ChatPerson",
                                "unselected": "lightgray",
                                "selected": "pink",
                                "scale": "1",
                                "tags": ["CHAT", "$ChatTopic"]
                              },
                              "children": [
                                {
                                  "type": "option",
                                  "props": {
                                    "name": "Me",
                                    "value": "Person.Me"
