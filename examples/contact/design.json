                                {
                                  "type": "option",
                                  "props": {
                                    "name": "Where",
                                    "value": "Aspect.Where"
                                  },
                                  "children": []
                                },
                                {
                                  "type": "option",
                                  "props": {
                                    "name": "Why",
                                    "value": "Aspect.Why"
                                  },
                                  "children": []
                                },
                                {
                                  "type": "option",
                                  "props": {
                                    "name": "How",
                                    "value": "Aspect.How"
                                  },
                                  "children": []
                                }
                              ]
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
