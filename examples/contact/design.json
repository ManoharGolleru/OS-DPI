{
  "type": "page",
  "props": {},
  "children": [
    {
      "type": "stack",
      "props": { "direction": "row", "background": "", "scale": "1" },
      "children": [
        {
          "type": "stack",
          "props": { "direction": "column", "background": "", "scale": "1" },
          "children": [
            {
              "type": "display",
              "props": {
                "stateName": "$Display",
                "background": "white",
                "scale": "1"
              },
              "children": []
            },
            {
              "type": "tab control",
              "props": {
                "stateName": "a-tab-control",
                "activeTab": "",
                "background": "",
                "scale": "6",
                "tabEdge": "bottom"
              },
              "children": [
                {
                  "type": "tab panel",
                  "props": {
                    "background": "#ffffbb",
                    "name": "Chat Menu",
                    "label": ""
                  },
                  "children": [
                    {
                      "type": "grid",
                      "props": {
                        "rows": "6",
                        "columns": "6",
                        "tags": ["MENU", "CHAT"],
                        "match": "contains",
                        "name": "chatMenu",
                        "background": "pale green",
                        "scale": "1"
                      },
                      "children": []
                    }
                  ]
                },
                {
                  "type": "tab panel",
                  "props": {
                    "background": "#ffffbb",
                    "name": "Task Menu",
                    "label": ""
                  },
                  "children": [
                    {
                      "type": "grid",
                      "props": {
                        "rows": "6",
                        "columns": "6",
                        "tags": ["MENU", "TASK"],
                        "match": "contains",
                        "name": "taskMenu",
                        "background": "pink",
                        "scale": "1"
                      },
                      "children": []
                    }
                  ]
                },
                {
                  "type": "tab panel",
                  "props": {
                    "background": "#f8d8b1",
                    "name": "Story Menu",
                    "label": ""
